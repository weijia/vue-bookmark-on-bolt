{
  "manifest_version": 3,
  "name": "Vue Bookmark on Bolt",
  "version": "1.0.0",
  "description": "A Vue.js based Edge extension",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}